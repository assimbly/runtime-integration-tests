<dil>
    <integrations>
        <integration>
            <flows>
                <flow>
                    <id>googledriveinbound_test1</id>
                    <name>googledriveinbound_test1</name>
                    <type>esb</type>
                    <steps>
                        <step>
                            <id>1</id>
                            <type>source</type>
                            <uri>googledrive</uri>
                            <options>
                                <accessToken>AAABBBCCCDDDEEEFFFGGHHH</accessToken>
                                <refreshToken>AAABBBCCCDDDEEEFFFGGHHH</refreshToken>
                                <directoryId>AAABBBCCCDDDEEEFFFGGHHH</directoryId>
                                <delay>1000</delay>
                                <initialDelay>1000</initialDelay>
                                <moveTo>.dovetail</moveTo>
                                <gSuiteFiles>Ignore</gSuiteFiles>
                            </options>
                        </step>
                        <step>
                            <id>2</id>
                            <type>action</type>
                            <uri>velocity:${bodyAs(String)}</uri>
                        </step>
                        <step>
                            <id>3</id>
                            <type>sink</type>
                            <uri>log</uri>
                        </step>
                    </steps>
                </flow>
            </flows>
        </integration>
    </integrations>
</dil>