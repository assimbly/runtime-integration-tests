{
  "dil": {
    "integrations": {
      "integration": {
        "flows": {
          "flow": {
            "id": "wiretap_test1",
            "name": "wiretap_test1",
            "type": "esb",
            "steps": {
              "step": [
                {
                  "id": "1",
                  "type": "source",
                  "uri": "https://0.0.0.0:9001/1/Wiretap"
                },
                {
                  "id": "2",
                  "type": "action",
                  "uri": "setbody:<persons>\t\t\t\t\t\t\t  <person>\t\t\t\t\t\t\t\t<name>John Doe</name>\t\t\t\t\t\t\t  </person>\t\t\t\t\t\t\t  <person>\t\t\t\t\t\t\t\t<name>Jane Doe</name>\t\t\t\t\t\t\t  </person>\t\t\t\t\t\t\t</persons>\t\t\t\t\t\t\t"
                },
                {
                  "id": "3",
                  "type": "action",
                  "uri": "wiretap:activemq:ID_1234"
                },
                {
                  "id": "4",
                  "type": "sink",
                  "uri": "velocity:Message Body: ${bodyAs(String)}"
                },
                {
                  "id": "5",
                  "type": "sink",
                  "uri": "log:Wiretap message",
                  "options": {
                    "skipBodyLineSeparator": "false",
                    "multiline": "true",
                    "showHeaders": "false",
                    "showBody": "true",
                    "showBodyType": "true",
                    "showFiles": "true",
                    "showException": "false",
                    "showStackTrace": "false"
                  },
                  "links": {
                    "link": {
                      "id": "ID_1234",
                      "transport": "activemq",
                      "bound": "in"
                    }
                  }
                }
              ]
            }
          }
        }
      }
    }
  }
}