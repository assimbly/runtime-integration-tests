                  "links": {
                    "link": [
                      {
                        "id": "3",
                        "transport": "sync",
                        "bound": "in"
                      },
                      {
                        "id": "4",
                        "transport": "sync",
                        "bound": "out"
                      }
                    ]
                  }
                },
                {
                  "id": "4",
                  "type": "sink",
